<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/bg_with_color_overlay"
	>

<!--	<ScrollView-->
<!--		android:id="@+id/vehicle_info"-->
<!--		android:layout_width="wrap_content"-->
<!--		android:layout_height="match_parent">-->

		<RelativeLayout
			android:id="@+id/page_vehicle_info_ek1"
			android:layout_marginTop="0dp"
			android:layout_marginStart="0dp"
			android:layout_width="wrap_content"
			android:layout_height="match_parent">


			<View
				android:id="@+id/rectangle_1_ek8"
				android:background="@drawable/_bg__call_popup_ek2_shape"
				android:layout_width="match_parent"
				android:layout_height="207dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="-17dp"
				android:scaleType="fitXY"/>

			<TextView
				android:id="@+id/garage_doc_ek4"
				android:layout_width="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_height="69.5dp"
				android:layout_marginStart="15dp"
				android:layout_marginTop="64dp"
				android:fontFamily="@font/ruslan_display"
				android:text="@string/garage_doc_ek4_string"
				android:textColor="@color/garage_doc_ek4_color"
				android:textSize="39sp" />

			<TableLayout
				android:id="@+id/bike_details_table"
				android:layout_marginStart="15dp"
				android:layout_marginTop="200dp"
				android:layout_width="335dp"
				android:layout_height="wrap_content">
				<TableRow>
					<TextView

						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="SN"
						android:textStyle="bold"
						android:padding="8dp" />
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Bike Details"
						android:textStyle="bold"
						android:padding="8dp" />
				</TableRow>
			</TableLayout>

			<ImageView
				android:id="@+id/image_1_ek1"
				android:src="@drawable/image_1_ek1"
				android:layout_centerHorizontal="true"
				android:contentDescription="image_1_ek1"
				android:layout_marginStart="155dp"
				android:layout_marginTop="388dp"
				android:layout_width="wrap_content"
				android:layout_height="49dp" />

			<TextView
				android:id="@+id/add_vehicles"
				android:layout_centerHorizontal="true"
				android:textSize="18sp"
				android:textColor="@color/rectangle_1_color"
				android:layout_marginStart="121dp"
				android:layout_marginTop="452dp"
				android:layout_width="wrap_content"
				android:layout_height="40dp"
				android:text="@string/add_vehicles_string" />


			</RelativeLayout>
	<RelativeLayout
		android:id="@+id/frame_23_ek16"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginStart="20dp"
		android:layout_marginEnd="20dp"
		android:layout_marginBottom="20dp"
		android:layout_gravity="bottom|center_horizontal"
		android:layout_alignParentBottom="true"
		android:background="@drawable/rectangle_1_ek10_shape">

		<ImageView
			android:id="@+id/vector_ek46"
			android:layout_width="29dp"
			android:layout_height="31dp"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="30dp"
			android:layout_marginTop="10dp"
			android:contentDescription="vector_ek46"
			android:src="@drawable/vector_ek46" />

		<ImageView
			android:id="@+id/vector_ek49"
			android:layout_width="30dp"
			android:layout_height="31dp"
			android:layout_alignParentTop="true"
			android:layout_marginStart="183dp"
			android:layout_marginTop="10dp"
			android:layout_toEndOf="@+id/vector_ek46"
			android:contentDescription="vector_ek49"
			android:src="@drawable/vector_ek49" />

		<ImageView
			android:id="@+id/_vector_ek50"
			android:layout_width="36dp"
			android:layout_height="31dp"
			android:layout_alignParentTop="true"
			android:layout_marginStart="70dp"
			android:layout_marginTop="10dp"
			android:layout_alignParentEnd="true"
			android:contentDescription="_vector_ek50"
			android:src="@drawable/_vector_ek50" />
		<ImageView
			android:id="@+id/_vector_ek51"
			android:layout_width="30dp"
			android:layout_height="36dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentStart="true"
			android:layout_marginStart="140dp"
			android:layout_marginTop="10dp"
			android:contentDescription="_vector_ek51"
			android:src="@drawable/_vector_ek51" />

	</RelativeLayout>
</FrameLayout>
