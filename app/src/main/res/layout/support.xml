<FrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/bg_with_color_overlay">


		<RelativeLayout
			android:id="@+id/page_support_ek1"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:layout_marginStart="0dp"
			android:layout_marginTop="0dp">

			<View
				android:id="@+id/rectangle_1_ek1"
				android:layout_width="match_parent"
				android:layout_height="212dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="-10dp"
				android:background="@drawable/rectangle_1_ek1_shape" />

			<TextView
				android:id="@+id/support_ek3"
				android:layout_width="153.5dp"
				android:layout_height="54.5dp"
				android:layout_marginLeft="22dp"
				android:layout_marginTop="102dp"
				android:text="@string/support_ek3_string"
				android:textColor="@color/_bg__extra_page__ek2_color"
				android:textSize="25sp"
				android:textStyle="bold" />

			<View
				android:id="@+id/rectangle_67"
				android:layout_width="344dp"
				android:layout_height="57.38dp"
				android:layout_marginStart="9dp"
				android:layout_marginTop="209dp"
				android:background="@drawable/rectangle_67_shape" />

			<ImageView
				android:id="@+id/rectangle_68"
				android:layout_width="344dp"
				android:layout_height="57.38dp"
				android:layout_marginStart="9dp"
				android:layout_marginTop="292.12dp"
				android:contentDescription="rectangle_68"
				android:src="@drawable/rectangle_68" />

			<ImageView
				android:id="@+id/rectangle_69"
				android:layout_width="344dp"
				android:layout_height="57.38dp"
				android:layout_marginStart="9dp"
				android:layout_marginTop="375dp"
				android:contentDescription="rectangle_69"
				android:src="@drawable/rectangle_69" />

			<ImageView
				android:id="@+id/rectangle_70"
				android:layout_width="344dp"
				android:layout_height="57.38dp"
				android:layout_marginStart="8dp"
				android:layout_marginTop="452dp"
				android:contentDescription="rectangle_70"
				android:src="@drawable/rectangle_70" />

			<ImageView
				android:id="@+id/people"
				android:layout_width="37dp"
				android:layout_height="32dp"
				android:layout_marginStart="19dp"
				android:layout_marginTop="222dp"
				android:contentDescription="people"
				android:src="@drawable/people" />

			<ImageView
				android:id="@+id/call_ek1"
				android:layout_width="25dp"
				android:layout_height="57dp"
				android:layout_marginStart="25dp"
				android:layout_marginTop="292dp"
				android:contentDescription="call_ek1"
				android:src="@drawable/call_ek1" />

			<ImageView
				android:id="@+id/encrypt"
				android:layout_width="68dp"
				android:layout_height="32dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="389dp"
				android:contentDescription="encrypt"
				android:src="@drawable/encrypt" />

			<ImageView
				android:id="@+id/terms_and_conditions"
				android:layout_width="28dp"
				android:layout_height="45dp"
				android:layout_marginStart="22dp"
				android:layout_marginTop="458dp"
				android:contentDescription="terms_and_conditions"
				android:src="@drawable/terms_and_conditions" />

			<TextView
				android:id="@+id/about_us"
				android:layout_width="115dp"
				android:layout_height="44dp"
				android:layout_marginLeft="68dp"
				android:layout_marginTop="222dp"
				android:text="@string/about_us_string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="20sp" />

			<TextView
				android:id="@+id/terms_and_condition"
				android:layout_width="235dp"
				android:layout_height="44dp"
				android:layout_marginLeft="68dp"
				android:layout_marginTop="466dp"
				android:text="@string/terms_and_condition_string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="20sp" />

			<TextView
				android:id="@+id/contact_us"
				android:layout_width="136dp"
				android:layout_height="44dp"
				android:layout_marginLeft="68dp"
				android:layout_marginTop="303dp"
				android:text="@string/contact_us_string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="20sp" />

			<TextView
				android:id="@+id/privacy_policy"
				android:layout_width="161dp"
				android:layout_height="44dp"
				android:layout_marginLeft="68dp"
				android:layout_marginTop="388dp"
				android:text="@string/privacy_policy_string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="20sp" />

			<ImageView
				android:id="@+id/_back_arrow"
				android:layout_width="39dp"
				android:layout_height="45dp"
				android:layout_marginStart="22dp"
				android:layout_marginTop="51dp"
				android:contentDescription="_back_arrow"
				android:src="@drawable/_back_arrow" />

		</RelativeLayout>

</FrameLayout>