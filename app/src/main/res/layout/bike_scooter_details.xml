<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/bg_with_color_overlay">

	<ScrollView
		android:id="@+id/bike_scooter_details"
		android:layout_width="match_parent"
		android:layout_height="match_parent">

		<RelativeLayout
			android:id="@+id/page_bike_scooter_details_ek1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="0dp"
			android:layout_marginStart="0dp">

			<View
				android:id="@+id/rectangle_1_ek4"
				android:layout_width="match_parent"
				android:layout_height="251dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:background="@drawable/_bg__call_popup_ek2_shape"
				android:scaleType="fitXY" />

			<TextView
				android:id="@+id/garage_doc_ek1"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_alignParentStart="true"
				android:layout_alignParentTop="true"
				android:layout_alignParentEnd="true"
				android:layout_alignBottom="@+id/rectangle_1_ek4"
				android:layout_marginStart="15dp"
				android:layout_marginTop="57dp"
				android:layout_marginEnd="97dp"
				android:layout_marginBottom="125dp"
				android:fontFamily="@font/ruslan_display"
				android:text="@string/garage_doc_ek1_string"
				android:textColor="@color/_986238999_color"
				android:textSize="39sp" />

			<View
				android:id="@+id/rectangle_93"
				android:layout_width="334dp"
				android:layout_height="46dp"
				android:layout_below="@+id/rectangle_1_ek4"
				android:layout_alignParentStart="true"
				android:layout_alignParentEnd="true"
				android:layout_marginStart="13dp"
				android:layout_marginTop="34dp"
				android:layout_marginEnd="64dp"
				android:background="@drawable/rectangle_93_shape" />

			<TextView
				android:id="@+id/bike_details"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_alignStart="@+id/rectangle_93"
				android:layout_alignEnd="@+id/rectangle_93"
				android:layout_alignBottom="@+id/rectangle_93"
				android:layout_marginStart="85dp"
				android:layout_marginTop="291dp"
				android:layout_marginEnd="76dp"
				android:layout_marginBottom="6dp"
				android:text="@string/bike_details_string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="25sp" />

			<TextView
				android:id="@+id/bike_model_"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_below="@+id/rectangle_93"
				android:layout_alignParentStart="true"
				android:layout_marginStart="12dp"
				android:layout_marginTop="32dp"
				android:text="@string/bike_model__string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="20sp" />

			<TextView
				android:id="@+id/number_plate_"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_below="@+id/bike_model_"
				android:layout_alignParentStart="true"
				android:layout_marginStart="12dp"
				android:layout_marginTop="27dp"
				android:text="@string/number_plate__string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="20sp" />

			<Spinner
				android:id="@+id/bikeModelEditText"
				android:layout_width="182dp"
				android:layout_height="35dp"
				android:layout_below="@+id/rectangle_93"
				android:layout_alignParentEnd="true"
				android:layout_marginStart="13dp"
				android:layout_marginTop="32dp"
				android:layout_marginEnd="74dp"
				android:layout_toEndOf="@+id/bike_model_"
				android:autofillHints=""
				android:hint="Mt-15"
				android:inputType="text"
				android:textColor="@color/black"
				android:textSize="15sp" />

			<EditText
				android:id="@+id/plateNumberEditText"
				android:layout_width="180dp"
				android:layout_height="35dp"
				android:layout_below="@+id/bikeModelEditText"
				android:layout_alignParentEnd="true"
				android:layout_marginStart="19dp"
				android:layout_marginTop="19dp"
				android:layout_marginEnd="73dp"
				android:autofillHints=""
				android:hint="BA-1-PA-123"
				android:inputType="text"
				android:textColor="@color/black"
				android:textSize="15sp" />

			<Button
				android:id="@+id/done"
				android:layout_width="200dp"
				android:layout_height="45dp"
				android:layout_below="@+id/plateNumberEditText"
				android:layout_alignParentStart="true"
				android:layout_alignParentEnd="true"
				android:layout_marginStart="90dp"
				android:layout_marginTop="19dp"
				android:layout_marginEnd="103dp"
				android:text="@string/done_string"
				android:textColor="@color/rectangle_1_color"
				android:textSize="15sp" />

		</RelativeLayout>
	</ScrollView>
</FrameLayout>
